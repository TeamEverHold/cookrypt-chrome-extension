{
  "manifest_version": 3,
  "name": "Cookrypt Background Service",
  "version": "1.0.0",
  "description": "A basic Cookrypt Background Service",
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": [
    "history"
  ],
  "externally_connectable": {
    "matches": ["<all_urls>"]
  }
}
